    <span class="<% IF pct_field_desc == 100 %>dbStatGood<% ELSE %>dbStatWarn<% END %>">Fields: <% pct_field_desc %>%</span>
  </div>

  <% IF smoketest %><div>

  </div><% END %>
</div>

<div id="page">
  <% IF doc_route %><p><a href="<% doc_route %>">Documentation</a></p><% END %>
  <pre>
<% route_config %>
  </pre>
</div>
